EN_DIR = en
RU_DIR = ru


##### Targets #####

install :
	$(MAKE) -C $(EN_DIR) install
	$(MAKE) -C $(RU_DIR) install

uninstall :
	$(MAKE) -C $(EN_DIR) uninstall
	$(MAKE) -C $(EN_DIR) uninstall


.PHONY : install uninstall
