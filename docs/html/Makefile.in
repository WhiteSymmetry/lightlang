MAKE=@MAKE_PROG@
CD=@CD_PROG@

EN_DIR=en
RU_DIR=ru

#####
install :
	$(CD) $(EN_DIR) && $(MAKE) install
	$(CD) $(RU_DIR) && $(MAKE) install

uninstall :
	$(CD) $(EN_DIR) && $(MAKE) uninstall
	$(CD) $(EN_DIR) && $(MAKE) uninstall

