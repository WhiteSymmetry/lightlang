MAKE=@MAKE_PROG@
CD=@CD_PROG@

DICTS_RESOURCES_DIR=dicts
SOUNDS_RESOURCES_DIR=sounds

#####
all :
	$(CD) $(DICTS_RESOURCES_DIR) && $(MAKE)

install :
	$(CD) $(DICTS_RESOURCES_DIR) && $(MAKE) install
	$(CD) $(SOUNDS_RESOURCES_DIR) && $(MAKE) install

uninstall :
	$(CD) $(DICTS_RESOURCES_DIR) && $(MAKE) uninstall
	$(CD) $(SOUNDS_RESOURCES_DIR) && $(MAKE) uninstall

clean :
	$(CD) $(DICTS_RESOURCES_DIR) && $(MAKE) clean

